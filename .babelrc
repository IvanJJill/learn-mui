{
  "presets": [
    [
      "@babel/env",
      {
        "targets": "last 2 versions",
        "useBuiltIns": "entry",
        "corejs": 2
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/transform-runtime",
    [
      "import",
      {
        "libraryName": "@material-ui/icons",
        "libraryDirectory": "", // default: lib
        "camel2DashComponentName": false // default: true
      },
    ],
  ]
}
